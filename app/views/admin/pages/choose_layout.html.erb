<% @page_title = "Skapa en ny sida" %>
<% @page_class = "AB content" -%>
<div id="workspace">
  <div class="header">
    <h1>Vilken typ av sida vill du skapa?</h1>
  </div>
  <div id="content">
    <ul>
    <% @page_layouts.each do |page_layout| -%>
      <li><%= link_to page_layout.name, new_admin_page_path(:page => { :page_layout_id => page_layout.id }) %></li>
    <% end -%>
    </ul>
  </div>
</div>
<%= javascript_include_tag 'named_routes' %>
<%= javascript_include_tag 'porthos/prototype', 'porthos/effects', 'porthos/controls', 'porthos/dragdrop', 'porthos/lowpro', 'porthos/lib', 'porthos/application', :cache => 'admin' %>
<%= javascript_include_tag 'porthos/pages' %>