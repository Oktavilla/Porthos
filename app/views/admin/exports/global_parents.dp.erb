<% @export.registrations.each do |g| -%>
<% list_code = ((g.conversion and g.conversion.measure_point and !g.conversion.measure_point.campaign.dp_global_parent_code.blank?) ? g.conversion.measure_point.campaign.dp_global_parent_code : 'WE') -%>
<%= [
  g.first_name.titlecase,
  g.last_name.titlecase,
  '',
  g.address.titlecase,
  g.post_code, # reformat 999 99
  g.locality.upcase,
  g.email,
  (g.contact_approval? ? 'HE' : ''),
  'P',
  'PRV',
  'WEBB',
  g.created_at.strftime("%Y-%m-%d"),
  g.come_from,
  list_code,
  'WE', # Byt ut banner id
  'VW',
  "1#{list_code[0..1]}#{g.total_sum.cents/1000}",
  g.total_sum.cents/100].collect { |value| "\"#{ value.to_s.gsub(/\n|\r/, '').to_latin1.chomp }\""}.join(',') %>
<% end -%>